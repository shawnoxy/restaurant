        <div id="page-wrapper">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        
                        <% if (option == 'breakfast') { %>
                            <%- include('includes/breakfast') -%>
                        <% } else if (option == 'brunch') { %>
                            <%- include('includes/brunch') -%>
                        <% } else if (option == 'lunch') { %>
                            <%- include('includes/lunch') -%>
                        <% } else if (option == 'dinner') { %>
