                        <% } else if (option == 'add_menu_item') { -%>
                            <%- include('includes/add_menu_item') -%>
                        <% } %>
                        
                        
                    </div>
                </div>
                <!-- /.row -->

            </div>
            <!-- /.container-fluid -->

        </div>
